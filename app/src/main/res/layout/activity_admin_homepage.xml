<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="15dp"
        android:text="Accounts"
        android:textSize="36sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/admin_homepage_textViewcreate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/textView"
        android:layout_marginTop="14dp"
        android:clickable="true"
        android:onClick="onClick"
        android:text="Create"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/admin_homepage_textViewedit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/admin_homepage_textViewcreate"
        android:layout_below="@+id/admin_homepage_textViewcreate"
        android:layout_marginTop="17dp"
        android:clickable="true"
        android:onClick="onClick"
        android:text="Edit"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/admin_homepage_textViewedit"
        android:layout_below="@+id/admin_homepage_textViewedit"
        android:layout_marginTop="37dp"
        android:text="Checklists"
        android:textSize="36sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/admin_homepage_textViewmodify"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="28dp"
        android:clickable="true"
        android:onClick="onClick"
        android:text="Modify"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/admin_homepage_textViewstatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/admin_homepage_textViewmodify"
        android:layout_below="@+id/admin_homepage_textViewmodify"
        android:layout_marginTop="21dp"
        android:clickable="true"
        android:onClick="onClick"
        android:text="View Status"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/admin_homepage_textViewchecklist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/admin_homepage_textViewstatus"
        android:layout_below="@+id/admin_homepage_textViewstatus"
        android:layout_marginTop="22dp"
        android:clickable="true"
        android:onClick="onClick"
        android:text="View Checklist"
        android:textSize="24sp" />

</RelativeLayout>