<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="highlighter.checklistapp.boundary.AdminSearchAccount">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/admin_search_account_search_filter"/>

    <ListView
        android:id="@+id/admin_search_account_search_accounts_list"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/admin_search_account_search_filter"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp"></ListView>
</RelativeLayout>
